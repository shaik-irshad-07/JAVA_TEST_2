class Student {

    String name;
    double marksObtained;

    static String collegeName = "LPU";
    static final double MAX_MARKS = 100;


    Student(String name, double marksObtained) {
        this.name = name;
        this.marksObtained = marksObtained;
    }


    public void printPercentage() {
        double percentage = (marksObtained / MAX_MARKS) * 100;
        System.out.printf("Percentage: %.2f%%\n", percentage);
    }

    public static void main(String[] args) {

        Student s1 = new Student("Irshad", 92);
        Student s2 = new Student("Nithish", 87);
        System.out.println("Name: " + s1.name);
        System.out.println("College: " + collegeName);
        System.out.println("Marks Obtained: " + s1.marksObtained);
        s1.printPercentage();
        System.out.println();
        System.out.println("Name: " + s2.name);
        System.out.println("College: " + collegeName);
        System.out.println("Marks Obtained: " + s2.marksObtained);
        s2.printPercentage();
    }
}


